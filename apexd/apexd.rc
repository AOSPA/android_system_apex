service apexd /system/bin/apexd
    class core
    user root
    group system
    shutdown critical
    disabled # does not start with the core class

service apexd-bootstrap /system/bin/apexd --bootstrap
    user root
    group system
    oneshot
    disabled

service apexd-snapshotde /system/bin/apexd --snapshotde
    user root
    group system
    oneshot
    disabled
